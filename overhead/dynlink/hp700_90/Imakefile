NormalObjectRule()
DependTarget()
LOCALINCLUDES=-I../common

CPPProgramTarget(adynlink, ATKOLinkI.o doloadi.o doload.o , ../common/libATKOLink.a ../common/libATKLink.a $(UTILLIB), )

CPPProgramTarget(aexelink,  ATKELinkI.o,  ../common/libATKELink.a ../common/libATKLink.a $(UTILLIB), )

InstallProgram(adynlink, $(DESTDIR)/etc)
InstallProgram(aexelink, $(DESTDIR)/etc)
LibraryTarget(libATKDynLoad.a, doloadi.o doload.o)
InstallLibrary(libATKDynLoad.a, $(DESTDIR)/lib)
