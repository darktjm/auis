addfiletype .c ctext
addfiletype .h ctext
addfiletype .C cpptext
addfiletype .H cpptext
addfiletype .org org
addfiletype .eq eq
addfiletype .fad fad
addfiletype .raster raster
addfiletype .table table
addfiletype .zip zip
addfiletype .fi figure
addfiletype .bdf bdffont
addfiletype .template text
addfiletype .overview text "template=help"
addfiletype .help text "template=help"
addfiletype * text "template=default"
# from samples
addfiletype .png imageio
addfiletype .gif imageio
addfiletype .jpg imageio
addfiletype .pbm imageio
addfiletype .ppm imageio
addfiletype .pnm imageio
addfiletype .xwd imageio
addfiletype .tif imageio
addfiletype .html htmltext "template=html"
addfiletype .letter text "template=letter"
addfiletype .memo text "template=memo"
# for champ
addfiletype .champ chimp

