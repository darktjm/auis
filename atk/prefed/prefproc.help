\chapter{Procedure Table Listing for prefed}


\proclistentry{\procname{prefsv-sort}(\proctype{prefsv} \procparm{obj}, \proctype{marker} \procparm{how})\
Sorts the preferences. (Valid \procparm{how} arguments are: Name, 
Group, App, Order)\
Menu:  \procmenu{Sort}\procmenuwhere{,}\procmenu{By application} ("App"); \procmenu{Sort}\procmenuwhere{,}\procmenu{By group} ("Group"); \procmenu{Sort}\procmenuwhere{~20}\procmenuwhere{,}\procmenu{By name} 
("Name"); \procmenu{Sort}\procmenuwhere{~20}\procmenuwhere{,}\procmenu{By order} ("Order")}


\proclistentry{\procname{pvaltv-update}(\proctype{pvaltv} \procparm{obj})\
sets the preference value from the text in the entry buffer.\
Bound to:  \prockey{Enter} (\prockey{^M})}


\proclistentry{\procname{pvaltvf-complete}(\proctype{pvaltvf} \procparm{obj})\
Attempts to complete the filename before the cursor.\
Bound to:  \prockey{Space}}

\proclistentry{\procname{pvaltvf-help}(\proctype{pvaltvf} \procparm{obj})\
Provides a list of possible completions in the choices list.\
Bound to:  \prockey{?}}


\proclistentry{\procname{pvaltvc-update}(\proctype{pvaltvc} \procparm{obj})\
sets the preference value from the text in the entry buffer.\
Bound to:  \prockey{Space}}


