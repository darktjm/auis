LOCALINCLUDES	=	-I${BASEDIR}/include/ams

PRODUCT	=	libcui.a

OBJS		= 	cuilib.o andmchs.o

NormalObjectRule()
LibraryTarget(${PRODUCT}, ${OBJS})
InstallLibrary(${PRODUCT}, ${DESTDIR}/lib)
DependTarget()

InstallDocs(CuiLib.pgr, ${DESTDIR}/doc/MAIL)
