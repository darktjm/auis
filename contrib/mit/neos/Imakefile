LOCALINCLUDES =  -I../fxlib/include/

NEOSLIBS = ../fxlib/lib/libfxcl.a ../fxlib/lib/libfxrpc.a


OBJS= blank.o eosbutton.o newbuttonview.o eframe.o papers.o papersaux.o eos.o \
	eosaux.o turnin.o turninaux.o pickup.o eosapp.o gradeapp.o eosfx.o

HFILES = blank.H eosbutton.H newbuttonview.H eframe.H papers.H \
	eos.H turnin.H pickup.H eosapp.H gradeapp.H eosfx.H

NormalObjectRule()

ATKLibraryTarget(neos,$(OBJS),$(NEOSLIBS),)
RunappWithLibrary(eos,neos,-r .)
RunappWithLibrary(grade,neos,-r .)

InstallPrograms(eos grade, $(DESTDIR)/bin)

NormalHelpRule()
InstallDocs(grade.help eos.help,${DESTDIR}/help)
