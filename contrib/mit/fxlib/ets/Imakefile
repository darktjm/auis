AnsiCC()

all:: rpc_err.o fxsrver_err.o fxcl_err.o krb_err.o

error_table(rpc_err)
error_table(fxsrver_err)
error_table(fxcl_err)
error_table(krb_err)

krb_err.et: mk_krb_err
	./mk_krb_err > $@

build_program(mk_krb_err,mk_krb_err.o,,-lkrb)

clean::
	rm -f core *.o *~ rpc_err.c fxsrver_err.c fxcl_err.c \
	krb_err.c krb_err.et mk_krb_err
