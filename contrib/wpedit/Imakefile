#ifdef AMS_DELIVERY_ENV
#ifdef WHITEPAGES_ENV


IHFILES		=	wpeditapp.H
OBJS		=	wpeditapp.o

LIBS =	 ${BASEDIR}/lib/libwpi.a \
		${BASEDIR}/lib/libmail.a \
		$(BASEDIR)/lib/libcparser.a \
		${UTILLIB} \
		${PRSLIB} 

DOCS		=	wpedit.help

NormalObjectRule()
NormalATKRule()
DependTarget()

DynamicObject(wpeditapp,$(LIBS),,)
InstallDynamicObjectClass(wpeditapp,$(FULLATKDLIBDIR))

OptionalATKLibrary(wpedit, $(OBJS), $(LIBS),,)
InstallOptionalATKLibrary(wpedit, $(DESTDIR)/lib/atk)

InstallClassFiles($(IHFILES))
InstallLink(runapp, ${DESTDIR}/bin/wpedit)
InstallDocs($(DOCS), $(DESTDIR)/help)

#endif WHITEPAGES_ENV
#endif AMS_DELIVERY_ENV
